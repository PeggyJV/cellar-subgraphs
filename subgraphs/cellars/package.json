{
  "name": "cellars",
  "version": "1.0.0",
  "description": "A Subgraph that tracks Cellars on Sommelier network",
  "main": "index.js",
  "scripts": {
    "build": "graph build",
    "codegen": "graph codegen",
    "create:local": "graph create --node http://localhost:8020 peggyjv/cellars",
    "remove:local": "graph remove --node http://localhost:8020 peggyjv/cellars",
    "deploy:local": "graph deploy --node http://localhost:8020 --ipfs http://localhost:5001 peggyjv/cellars",
    "deploy:hosted": "graph deploy --product hosted-service peggyjv/cellars",
    "deploy:hosted:test": "graph deploy --product hosted-service elkdao/cellarsnext",
    "create:prod": "graph create --node http://localhost:9020 peggyjv/cellars",
    "deploy:prod": "graph deploy --node http://localhost:9020 --ipfs https://ipfs.network.thegraph.com/ipfs peggyjv/cellars",
    "create:prod:b": "graph create --node http://localhost:9020 peggyjv/cellars-b",
    "remove:prod:b": "graph remove --node http://localhost:9020 peggyjv/cellars-b",
    "deploy:prod:b": "graph deploy --node http://localhost:9020 --ipfs https://ipfs.network.thegraph.com/ipfs peggyjv/cellars-b",
    "test": "graph test",
    "graph:version": "graph -v"
  }
}
